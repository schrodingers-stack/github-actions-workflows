{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":semanticCommitTypeAll(chore)",
    "docker:pinDigests",
    "docker:enableMajor"
  ],
  "platform": "github",
  "platformCommit": "enabled",
  "repositoryCache": "enabled",
  "autodiscover": true,
  "onboarding": false,
  "requireConfig": "required",
  "branchPrefix": "renovate-github/",
  "labels": [
    "type: chore"
  ]
}
